 &END SCF
 &XC
 &XC_FUNCTIONAL
 &PBE
 &END PBE
 &END XC_FUNCTIONAL
 &XC_GRID
 XC_DERIV SPLINE2_smooth
 XC_SMOOTH_RHO NN10
 &END XC_GRID
 &END XC
 &END DFT
 &SUBSYS
 &CELL
 PERIODIC XYZ
 ABC 8. 8. 8.
 &END CELL
 &COORD
 O 0.000000 0.000000 -0.065587
 H 0.000000 -0.757136 0.520545
 H 0.000000 0.757136 0.520545
 &END COORD
 &KIND H
 BASIS_SET DZVP-GTH-PBE
 POTENTIAL GTH-PBE-q1
 &END KIND
 &KIND O
 BASIS_SET DZVP-GTH-PBE
 POTENTIAL GTH-PBE-q6
 &END KIND
 &END SUBSYS
&END FORCE_EVAL
